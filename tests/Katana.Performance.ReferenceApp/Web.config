<configuration>
  <appSettings>
    <!--<add key="owin:Configuration" value="MyStartup"/>-->

    <add key="owin:Application" value="Startup.cs" />
    <add key="owin:ApplicationLoaders" value="Microsoft.Owin.Hosting;Extra.Roslyn;Extra.CSharp" />
    <add key="owin:ApplicationWrappers" value="one;bar" />

    <add key="owin:HandleAllRequests" value="true" />
    <add key="owin:SetCurrentDirectory" value="true" />
  </appSettings>

  <!--
    For a description of web.config changes for .NET 4.5 see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
  -->
  <system.web>
    <httpRuntime targetFramework="4.5" />
    <compilation debug="true" targetFramework="4.5"/>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
</configuration>